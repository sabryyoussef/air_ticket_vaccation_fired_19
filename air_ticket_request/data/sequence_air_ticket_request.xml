<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequence for Air Ticket Requests -->
        <record id="seq_air_ticket_request" model="ir.sequence">
            <field name="name">Air Ticket Request Sequence</field>
            <field name="code">air.ticket.request</field>
            <field name="prefix">ATR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
          <!-- Sequence for Air Ticket Types -->
        <record id="seq_air_ticket_request" model="ir.sequence">
            <field name="name">Air Ticket type Sequence</field>
            <field name="code">air.ticket.type</field>
            <field name="prefix">ATT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>